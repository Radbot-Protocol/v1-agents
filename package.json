  "type": "module",
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "4.0.1",
    "@nomicfoundation/hardhat-ethers-chai-matchers": "3.0.0",
    "@nomicfoundation/hardhat-ignition": "3.0.0",
    "@nomicfoundation/hardhat-ignition-ethers": "3.0.3",
    "@nomicfoundation/hardhat-keystore": "3.0.1",
    "@nomicfoundation/hardhat-mocha": "3.0.2",
    "@nomicfoundation/hardhat-network-helpers": "3.0.0",
    "@nomicfoundation/hardhat-toolbox-mocha-ethers": "3.0.0",
    "@nomicfoundation/hardhat-typechain": "3.0.0",
    "@nomicfoundation/hardhat-verify": "3.0.2",
    "@solidstate/hardhat-contract-sizer": "3.0.0",
    "@types/chai": "4.2.0",
    "@types/chai-as-promised": "8.0.1",
    "@types/mocha": "10.0.10",
    "@types/node": "22.8.5",
    "chai": "5.1.2",
    "dotenv": "^17.2.2",
    "ethers": "6.14.0",
    "forge-std": "foundry-rs/forge-std#v1.9.4",
    "hardhat": "3.0.6",
    "husky": "^9.1.7",
    "mocha": "11.0.0",
    "typescript": "~5.8.0"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "hardhat clean && rm -rf types/",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy:deployer:base": "npx hardhat ignition deploy ignition/modules/RadbotV1Deployer.ts  --network base",
    "deploy:factory:base": "npx hardhat ignition deploy ignition/modules/RadbotV1Factory.ts  --network base",
    "init:factory:base": "npx hardhat run scripts/init-factory.ts --network base",
    "init:deployer:base": "npx hardhat run scripts/init-deployer.ts --network base",
    "deploy:agent:base": "npx hardhat run scripts/deploy-agent.ts --network base",
    "set:fee:base": "npx hardhat run scripts/setFee.ts --network base"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0"
  }
}
